struct embed
{
    int32 a;
    int32 b;
    int32 c;
}

struct test
{
    embed first = embed{ 1, 2, -0x3, };
    embed second = { 4, c = 6 };
}

// CHECK: test.first.@type is embed
// CHECK: test.first.@default.@type is embed

// CHECK: test.first.@fields is 3
// CHECK: test.first.@default.a is 1
// CHECK: test.first.@default.b is 2
// CHECK: test.first.@default.c is -3

// CHECK: test.second.@type is-a embed
// CHECK: test.second.@default.@fields is 2
// CHECK: test.second.@default.a is 4
// CHECK: test.second.@default.c is 6
